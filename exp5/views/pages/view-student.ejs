<%- include('../includes/header') %>
<div class="container" style="min-height: 700px; margin-top: 100px !important;">
    
    <div class="row">
        <div class="col-md-8 offset-md-2">
            <h3 class="text-center">List of All Student</h3>
            <table class="table table-dark table-bordered table-striped">
                <tr>
                    <th>S.No.</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Address</th>
                    <th>City</th>
                </tr>
                <% result.forEach((item, i)=>{ %>
                    <tr>
                        <td><%= i+1 %></td>
                        <td><%= item.name %></td>
                        <td><%= item.email %></td>
                        <td><%= item.address %></td>
                        <td><%= item.city %></td>
                    </tr>
                <% }) %>
            </table>
        </div>
    </div>
    
</div>
<%- include('../includes/footer') %>