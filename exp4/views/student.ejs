<%- include('header') %>
<div class="container my-4">
    <div class="row">
        <div class="col-md-12">
            <h3>List of All Student</h3>
<!--             
            <% color.forEach((item, n)=>{ %>
                <h1><%= n+"-"+item %></h1>
            <% }) %> -->
            
            <table class="table table-dark table-hover table-stripped table-bordered">
                <tr>
                    <th>S.No.</th>
                    <th>Name</th>
                    <th>Age</th>
                    <th>City</th>
                    <th>Fee</th>
                </tr>
                <% user.forEach((u, i)=>{ 
                    
                    if(u.age >=25){
                    
                    %>
                    <tr>
                        <td><%= i+1 %></td>
                        <td><%= u.name %></td>
                        <td><%= u.age %></td>
                        <td><%= u.city %></td>
                        <td><%= u.fee %></td>
                    </tr>
                <% 
                }
            }) %>
                
            </table>

            

        </div>
    </div>
</div>
<%- include('footer') %>