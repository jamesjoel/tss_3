<%- include('../includes/header')  %>
<div class="container" style="min-height: 700px;">
    <div class="row">
        <div class="col-md-12 mt-5" >

            <h1>Record</h1>

            <table class="table table-hover table-stripped table-bordered table-warning">
                <tr>
                    <th>sno.</th>
                    <th>name</th>
                    <th>age</th>
                    <th>city</th>               
                </tr>
                <% user.forEach((item,i){ %>
                    <tr>
                        <td><%= i+1   %></td>
                        <td><%= item.name %></td>
                        <td><%= item.age  %></td>
                        <td><%= item.city %></td>
                    </tr>

                <% }) %>
            </table>
        </div>
    </div>
</div>
 

<%- include('../includes/footer')  %>